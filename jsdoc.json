{
  "opts": {
    "template": "node_modules/better-docs",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "tutorials": "./tutorials"
  },
  "tags": {
    "allowUnknownTags": ["category"]
  },
  "plugins": [
    "node_modules/better-docs/category"
  ],
  "source": {
    "include": ["./", "./README.md"],
    "exclude": ["./node_modules", "./docs", "./tutorials", "./sia.js"]
  }
}